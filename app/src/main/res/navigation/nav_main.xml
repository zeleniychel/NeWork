<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/postsFeedFragment">
    <fragment
        android:id="@+id/postsFeedFragment"
        android:name="ru.netology.nework.activity.PostsFeedFragment"
        android:label="PostsFeedFragment" >
        <action
            android:id="@+id/action_postsFeedFragment_to_postFragment"
            app:destination="@id/postFragment" />
        <action
            android:id="@+id/action_postsFeedFragment_to_newPostFragment"
            app:destination="@id/newPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="ru.netology.nework.activity.SignInFragment"
        android:label="SignInFragment" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="ru.netology.nework.activity.SignUpFragment"
        android:label="SignUpFragment" />
    <fragment
        android:id="@+id/usersFeedFragment"
        android:name="ru.netology.nework.activity.UsersFeedFragment"
        android:label="UsersFeedFragment" >
        <action
            android:id="@+id/action_usersFeedFragment_to_userWallFragment"
            app:destination="@id/userWallFragment" />
        <action
            android:id="@+id/action_usersFeedFragment_to_myWallFragment"
            app:destination="@id/myWallFragment" />
    </fragment>
    <fragment
        android:id="@+id/eventFeedFragment"
        android:name="ru.netology.nework.activity.EventFeedFragment"
        android:label="EventFeedFragment" >
        <action
            android:id="@+id/action_eventFeedFragment_to_eventFragment"
            app:destination="@id/eventFragment" />
        <action
            android:id="@+id/action_eventFeedFragment_to_newEventFragment"
            app:destination="@id/newEventFragment" />
    </fragment>
    <fragment
        android:id="@+id/postFragment"
        android:name="ru.netology.nework.activity.PostFragment"
        android:label="PostFragment" >
        <action
            android:id="@+id/action_postFragment_to_usersFragment"
            app:destination="@id/usersFragment" />
    </fragment>
    <fragment
        android:id="@+id/usersFragment"
        android:name="ru.netology.nework.activity.UsersFragment"
        android:label="UsersFragment" />
    <fragment
        android:id="@+id/eventFragment"
        android:name="ru.netology.nework.activity.EventFragment"
        android:label="EventFragment" >
        <action
            android:id="@+id/action_eventFragment_to_usersFragment"
            app:destination="@id/usersFragment" />
    </fragment>
    <fragment
        android:id="@+id/newPostFragment"
        android:name="ru.netology.nework.activity.NewPostFragment"
        android:label="NewPostFragment" >
        <action
            android:id="@+id/action_newPostFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_newPostFragment_to_checkUsersFragment"
            app:destination="@id/checkUsersFragment" />
    </fragment>
    <fragment
        android:id="@+id/newEventFragment"
        android:name="ru.netology.nework.activity.NewEventFragment"
        android:label="NewEventFragment" >
        <action
            android:id="@+id/action_newEventFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_newEventFragment_to_checkUsersFragment"
            app:destination="@id/checkUsersFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="ru.netology.nework.activity.MapFragment"
        android:label="MapFragment" />
    <fragment
        android:id="@+id/userWallFragment"
        android:name="ru.netology.nework.activity.WallFragment"
        android:label="UserWallFragment" >
        <action
            android:id="@+id/action_userWallFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/myWallFragment"
        android:name="ru.netology.nework.activity.MyWallFragment"
        android:label="MyWallFragment" >
        <action
            android:id="@+id/action_myWallFragment_to_newJobFragment"
            app:destination="@id/newJobFragment" />
        <action
            android:id="@+id/action_myWallFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/newJobFragment"
        android:name="ru.netology.nework.activity.NewJobFragment"
        android:label="NewJobFragment" />
    <fragment
        android:id="@+id/checkUsersFragment"
        android:name="ru.netology.nework.activity.CheckUsersFragment"
        android:label="CheckUsersFragment" />
</navigation>